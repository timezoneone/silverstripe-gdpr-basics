{
    "name": "timezoneone/silverstripe-gdpr-basics",
    "description": "SilverStripe GDPR Cookie Policy Module",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/timezoneone/silverstripe-gdpr-basics",
    "require": {
        "silverstripe/cms": "^4 || ^5",
        "silverstripe/framework": "^4 || ^5",
        "unclecheese/display-logic": "^2 || ^3",
        "tractorcow/silverstripe-colorpicker": "^4.0.0",
        "geoip2/geoip2": "^2.9.0",
        "silverstripe/userforms": "^5 || ^6"
    },
    "autoload": {
        "psr-4": {
            "TimeZoneOne\\GDPR\\": "src/"
        }
    },
    "extra": {
        "installer-name": "gdpr-basics",
        "expose": [
            "client/dist",
            "client/images"
        ]

    }
}
